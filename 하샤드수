class Solution {
    public boolean solution(int x) {
        boolean answer = true;
        int res = 0;
        int xx = x;
        while(x > 0)
        {
            res += x%10;
            x /= 10;
        }
        
        if(xx % res == 0) answer = true;
        else answer = false;
        
        return answer;
    }
}
